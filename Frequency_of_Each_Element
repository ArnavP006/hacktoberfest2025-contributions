#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];
    int freq[1000] = {0};

    printf("Enter %d numbers:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        freq[arr[i]]++;
    }

    printf("Element frequencies:\n");
    for (int i = 0; i < n; i++) {
        if (freq[arr[i]] != 0) {
            printf("%d -> %d\n", arr[i], freq[arr[i]]);
            freq[arr[i]] = 0;
        }
    }

    return 0;
}
